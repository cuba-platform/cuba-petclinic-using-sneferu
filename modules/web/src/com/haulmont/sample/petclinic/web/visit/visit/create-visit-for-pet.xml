<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
  caption="msg://caption"
  messagesPack="com.haulmont.sample.petclinic.web.visit.visit">
  <actions>
    <action id="createVisit"
      caption="msg://create"
      primary="true"
    />
  </actions>
  <layout spacing="true">
    <textField
      id="identificationNumber"
      required="true"
      requiredMessage="msg://identificationNumberRequired"
      caption="msg://identificationNumber"/>
    <buttonsPanel>
      <button
        id="createVisitBtn"
        action="createVisit"
      />
    </buttonsPanel>
  </layout>
</window>
